{
  "require": {
    "craftcms/cms": "3.7.23",
    "craftcms/element-api": "^2.8",
    "craftcms/redactor": "2.8.8",
    "craftsnippets/craft-image-toolbox": "^1.1",
    "misterbk/mix": "^1.5",
    "nystudio107/craft-minify": "^1.2",
    "sebastianlenz/linkfield": "^1.0",
    "verbb/super-table": "2.7.0",
    "vlucas/phpdotenv": "^3.4.0",
    "yiisoft/yii2-redis": "^2.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.5"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
